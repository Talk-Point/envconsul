{
    "name": "talk-point/envconsul",
    "description": "Get env variables from Consul service",
    "keywords": [
        "talk-point",
        "envonsul"
    ],
    "homepage": "https://github.com/Talk-Point/envconsul",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Stoldt",
            "email": "konstantin.stoldt@talk-point.de",
            "homepage": "https://github.com/Talk-Point/envconsul",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "TPConsul\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPConsul\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
